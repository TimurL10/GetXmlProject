
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Жнвлс Отчет</h1>

<form method="post" enctype="multipart/form-data" asp-controller="Drug" asp-action="Index">
    <div class="form-group">
        <div class="col-md-10">
            <p>Выберите файлы Надбавок и список Жнвлс:</p>
            <input type="file" name="fileList" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>

<div class="form-group">
    <div class="col-md-10">
        <a type="submit" asp-action="Start" asp-controller="Drug" asp-antiforgery="true">Старт</a>
    </div>
</div>

    

    
