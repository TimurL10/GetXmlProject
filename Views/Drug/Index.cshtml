
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Жнвлс Отчет</h1>

<form method="post" enctype="multipart/form-data" asp-controller="Drug" asp-action="Index">
    <div class="form-group">
        <div class="col-md-10">
            <p>Загрузите файл Надбавок:</p>
            <input type="file" name="files" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>

<p>
    <a asp-action="StartProc" asp-controller="Drug" asp-antiforgery="true">Start</a>
</p>